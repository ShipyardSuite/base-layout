{
	"name": "@shipyardsuite/base-layout",
	"version": "0.2.22",
	"description": "Shared components for Shipyard GCDN Microservices",
	"author": "ShipyardSuite",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ShipyardSuite/base-layout.git"
	},
	"main": "example/server/index.js",
	"module": "src/index.js",
	"jsnext:main": "src/index.js",
	"engines": {
		"node": ">=8",
		"npm": ">=5"
	},
	"scripts": {
		"build": "webpack --mode production",
		"start": "npm run build && node example/server/index.js",
		"client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
		"server": "nodemon example/server/index.js",
		"build:publish": "npm build && npm publish --access public",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"express": "^4.16.3",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"semantic-ui-react": "^0.88.2"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"semantic-ui-react": "^0.88.2",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.0",
		"babel-loader": "^8.0.0",
		"clean-webpack-plugin": "^1.0.0",
		"concurrently": "^4.0.0",
		"css-loader": "^2.0.0",
		"eslint": "^5.0.0",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0",
		"file-loader": "^3.0.0",
		"html-webpack-plugin": "^3.2.0",
		"nodemon": "^1.17.3",
		"style-loader": "^0.23.0",
		"url-loader": "^1.0.1",
		"webpack": "^4.5.0",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.3"
	},
	"files": [ "src" ],
	"bugs": {
		"url": "https://github.com/ShipyardSuite/base-layout/issues"
	},
	"homepage": "https://github.com/ShipyardSuite/base-layout#readme",
	"directories": {
		"example": "example"
	}
}
